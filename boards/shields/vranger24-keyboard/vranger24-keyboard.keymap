/*
 * VKey29 keymap â€” 29 keys per half.
 * Order: num(4) | top(6) | home(6) | bottom(6) | thumb(7).
 */
#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>

/ {
	keymap {
		compatible = "zmk,keymap";

		default_layer {
			bindings = <
				/* num: 4 */
				&kp N1   &kp N2   &kp N3   &kp N4
				/* top: 6 */
				&kp Q    &kp W    &kp E    &kp R    &kp T    &kp Y
				/* home: 6 */
				&kp A    &kp S    &kp D    &kp F    &kp G    &kp H
				/* bottom: 6 */
				&kp Z    &kp X    &kp C    &kp V    &kp B    &kp N
				/* thumb: 7 */
				&kp LCTL &kp LALT &kp LGUI &kp SPACE &kp ENTER &kp TAB &kp ESC
			>;
		};
	};
};
